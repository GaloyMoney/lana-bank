        "approval_process_id": {
          "format": "uuid",
          "type": "string"
        },
        "disbursal_credit_account_id": {
          "format": "uuid",
          "type": "string"
        },
        "due_date": {
          "$ref": "#/$defs/EffectiveDate"
        },
        "facility_id": {
          "format": "uuid",
          "type": "string"
        },
        "id": {
          "format": "uuid",
          "type": "string"
        },
        "ledger_tx_id": {
          "format": "uuid",
          "type": "string"
        },
        "liquidation_date": {
