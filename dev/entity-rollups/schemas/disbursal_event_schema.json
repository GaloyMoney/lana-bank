        "overdue_date": {
          "anyOf": [
            {
              "$ref": "#/$defs/EffectiveDate"
            },
            {
              "type": "null"
            }
          ]
        },
        "public_id": {
          "type": "string"
        },
        "type": {
          "const": "initialized",
          "type": "string"
        }
      },
      "required": [
        "type",
        "id",
        "ledger_tx_id",
        "approval_process_id",
        "facility_id",
        "amount",
        "account_ids",
        "disbursal_credit_account_id",
        "due_date",
        "public_id"
