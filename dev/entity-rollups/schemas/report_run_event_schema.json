{
  "$defs": {
    "ReportRunState": {
      "enum": [
        "queued",
        "running",
        "success",
        "failed"
      ],
      "type": "string"
    },
    "ReportRunType": {
      "enum": [
        "scheduled",
        "manual"
      ],
      "type": "string"
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "oneOf": [
    {
      "properties": {
        "external_id": {
          "type": "string"
        },
        "id": {
          "format": "uuid",
          "type": "string"
        },
        "run_type": {
          "$ref": "#/$defs/ReportRunType"
        },
        "start_time": {
          "format": "date-time",
          "type": [
            "string",
            "null"
          ]
        },
        "state": {
          "$ref": "#/$defs/ReportRunState"
        },
        "type": {
          "const": "initialized",
          "type": "string"
        }
      },
      "required": [
        "type",
        "id",
        "external_id",
        "state",
        "run_type"
      ],
      "type": "object"
    },
    {
      "properties": {
        "run_type": {
          "$ref": "#/$defs/ReportRunType"
        },
        "start_time": {
          "format": "date-time",
          "type": [
            "string",
            "null"
          ]
        },
        "state": {
          "$ref": "#/$defs/ReportRunState"
        },
        "type": {
          "const": "state_updated",
          "type": "string"
        }
      },
      "required": [
        "type",
        "state",
        "run_type"
      ],
      "type": "object"
    }
  ],
  "title": "ReportRunEvent"
}
