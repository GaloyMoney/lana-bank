{
  "$defs": {
    "UsdCents": {
      "format": "uint64",
      "minimum": 0,
      "type": "integer"
    },
    "WithdrawalStatus": {
      "enum": [
        "PendingApproval",
        "PendingConfirmation",
        "Confirmed",
        "Denied",
        "Cancelled",
        "Reverted"
      ],
      "type": "string"
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "oneOf": [
    {
      "properties": {
        "amount": {
          "$ref": "#/$defs/UsdCents"
        },
        "approval_process_id": {
          "format": "uuid",
          "type": "string"
        },
        "deposit_account_id": {
          "format": "uuid",
          "type": "string"
        },
        "id": {
          "format": "uuid",
          "type": "string"
        },
        "ledger_tx_id": {
          "format": "uuid",
          "type": "string"
        },
        "reference": {
          "type": "string"
        },
        "status": {
          "$ref": "#/$defs/WithdrawalStatus"
        },
        "type": {
          "const": "initialized",
          "type": "string"
        }
      },
      "required": [
        "type",
        "id",
        "ledger_tx_id",
        "deposit_account_id",
        "amount",
        "reference",
        "approval_process_id",
        "status"
      ],
      "type": "object"
    },
    {
      "properties": {
        "approval_process_id": {
          "format": "uuid",
          "type": "string"
        },
        "approved": {
          "type": "boolean"
        },
        "status": {
          "$ref": "#/$defs/WithdrawalStatus"
        },
        "type": {
          "const": "approval_process_concluded",
          "type": "string"
        }
      },
      "required": [
        "type",
        "approval_process_id",
        "approved",
        "status"
      ],
      "type": "object"
    },
    {
      "properties": {
        "ledger_tx_id": {
          "format": "uuid",
          "type": "string"
        },
        "status": {
          "$ref": "#/$defs/WithdrawalStatus"
        },
        "type": {
          "const": "confirmed",
          "type": "string"
        }
      },
      "required": [
        "type",
        "ledger_tx_id",
        "status"
      ],
      "type": "object"
    },
    {
      "properties": {
        "ledger_tx_id": {
          "format": "uuid",
          "type": "string"
        },
        "status": {
          "$ref": "#/$defs/WithdrawalStatus"
        },
        "type": {
          "const": "cancelled",
          "type": "string"
        }
      },
      "required": [
        "type",
        "ledger_tx_id",
        "status"
      ],
      "type": "object"
    },
    {
      "properties": {
        "ledger_tx_id": {
          "format": "uuid",
          "type": "string"
        },
        "status": {
          "$ref": "#/$defs/WithdrawalStatus"
        },
        "type": {
          "const": "reverted",
          "type": "string"
        }
      },
      "required": [
        "type",
        "ledger_tx_id",
        "status"
      ],
      "type": "object"
    }
  ],
  "title": "WithdrawalEvent"
}
