    "apollo-upload-client": "^18.0.1",
    "bignumber.js": "^9.3.1",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "keycloak-js": "^26.2.1",
    "lucide-react": "^0.556.0",
    "next": "16.0.10",
    "next-intl": "^4.5.8",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-idle-timer": "^5.7.2",
