{
  "name": "admin-panel",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "next dev --port 3001 --turbopack",
    "validate-translations": "node ./i18n/validate-translations.mjs",
    "build": "npm run validate-translations && next build",
    "start": "next start",
    "lint": "next lint",
    "tsc-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:fix": "eslint --max-warnings 0 --fix --ext .ts,.tsx .",
    "cypress:run": "./cypress/run.sh",
    "cypress:run-local": "COOKIES=$COOKIES cypress open --env COOKIES=$COOKIES",
    "cypress:run-headless": "COOKIES=$COOKIES cypress run  --env COOKIES=$COOKIES",
    "cypress:run-browserstack": "COOKIES=$COOKIES browserstack-cypress run  --env COOKIES=$COOKIES",
    "local:pdf": "cd cypress/manuals && ./local-pdf.sh"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/client-integration-nextjs": "^0.12.2",
    "@faker-js/faker": "^9.8.0",
    "@lana/web": "workspace:*",
    "@ory/client": "^1.20.11",
    "@t3-oss/env-nextjs": "^0.13.6",
    "apollo-upload-client": "^18.0.1",
    "axios": "^1.9.0",
    "bignumber.js": "^9.3.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-intl": "^4.1.0",
    "react": "^19.1.0",
    "react-apexcharts": "^1.5.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-number-format": "^5.4.4",
    "react-qr-code": "^2.0.14",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0-next.16",
    "@faker-js/faker": "^9.8.0",
    "@graphql-codegen/cli": "5.0.6",
