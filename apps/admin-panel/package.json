{
  "name": "admin-panel",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "next dev --port 3001 --turbopack",
    "validate-translations": "node ./i18n/validate-translations.mjs",
    "build": "npm run validate-translations && next build",
    "start": "next start",
    "lint": "eslint --max-warnings 0 --ext .ts,.tsx .",
    "tsc-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:fix": "eslint --max-warnings 0 --fix --ext .ts,.tsx .",
    "cypress:run-ui": "cypress open",
    "cypress:run-headless": "cypress run",
    "local:pdf": "cd cypress/manuals && ./local-pdf.sh"
  },
  "dependencies": {
