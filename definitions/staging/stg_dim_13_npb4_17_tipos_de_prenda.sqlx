config {
	type: "table",
}

WITH tipos_de_prenda AS (

    SELECT '01' AS code, 'Mercadería perecedera o sujeta a obsolescencia en el corto plazo' AS description UNION ALL
    SELECT '02', 'Mercadería no perecedera o no sujeta a obsolescencia en el corto plazo'

)


SELECT
    code,
    description
FROM tipos_de_prenda
