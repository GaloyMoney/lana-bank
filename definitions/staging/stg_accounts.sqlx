config {
	type: "table",
}

SELECT
	id AS account_id,
	JSON_VALUE(ANY_VALUE(values HAVING MAX recorded_at), "$.name") AS account_name,
	JSON_VALUE(ANY_VALUE(values HAVING MAX recorded_at), "$.normal_balance_type") AS normal_balance_type,

FROM ${ref({
	name: "accounts",
	schema: envs.currentImportSchema
})}

GROUP BY id
