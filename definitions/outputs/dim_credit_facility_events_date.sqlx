config {
	type: "table",
	description: "Each row represents a unique event date in credit facility.",
	assertions: {
		uniqueKey: ["PK"],
		nonNull: ["event_date"],
	},
}

SELECT
      CAST(FORMAT_DATE('%Y%m%d', event_date) as INT64) AS PK
    , event_date
FROM
	UNNEST(
		GENERATE_DATE_ARRAY(
			'2024-01-01',
			'2140-01-01',
			INTERVAL 1 DAY)
	) as event_date
