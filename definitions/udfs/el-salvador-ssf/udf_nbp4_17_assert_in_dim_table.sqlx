      WHEN dim_table_name = "stg_dim_32_npb4_17_programa_de_asistencia_tecnica"           THEN COALESCE(assert_value, '~') NOT IN (SELECT `code` FROM ${ref('stg_dim_32_npb4_17_programa_de_asistencia_tecnica')})
      ELSE FALSE
    END
  )
);
