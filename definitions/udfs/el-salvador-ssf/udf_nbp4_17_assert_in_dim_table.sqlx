config {
	type: "operations",
	hasOutput: true,
	description: "Asserts that the value is a valid code in the static data table."
}

CREATE OR REPLACE FUNCTION ${self()} (assert_value STRING, dim_table_name STRING)
RETURNS BOOLEAN
AS (
  (
    CASE
      WHEN dim_table_name = "stg_dim_01_npb4_17_tipos_de_cartera"                         THEN COALESCE(assert_value, '~') NOT IN (SELECT `code` FROM ${ref('stg_dim_01_npb4_17_tipos_de_cartera')})
      WHEN dim_table_name = "stg_dim_02_npb4_17_tipos_de_activos_de_riesgo"               THEN COALESCE(assert_value, '~') NOT IN (SELECT `code` FROM ${ref('stg_dim_02_npb4_17_tipos_de_activos_de_riesgo')})
